{
    "daq_application" : {
        "cmd": [
            "CMD_FAC=rest://localhost:${APP_PORT}",
            "INFO_SVC=file://info_${APP_NAME}_${APP_PORT}.json",
            "cd ${DBT_AREA_ROOT}",
            "source dbt-setup-env.sh",
            "dbt-setup-runtime-environment",
            "cd ${NANO_SESSION_DIR}",
            "daq_application --name ${APP_NAME} -c ${CMD_FAC} -i ${INFO_SVC}"
        ]
    }
}